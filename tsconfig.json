{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["vite/client", "@lightningjs/vite-plugin-import-chunk-url/client"],
    "noEmit": true,
    "isolatedModules": true,
    "noImplicitAny": false,
    "resolveJsonModule": true
  },
  "exclude": [
    "src/components/GenerateComponents.ts",
    "src/components/GenerateIndex.ts"
  ]
}
